<?xml version="1.0" encoding="utf-8"?>  
<openerp>
    <data>
        <record model="ir.ui.view" id="view_product_template_product_form_dfn_inherit">
            <field name="name">ICE Product Product Inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//page[@string='Information']/group[1]/group[1]/field[@name='type']" position="before">
                                <field name="productname" readonly="1"/>
                                <field name="drawingfilename" readonly="1"/>
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</openerp>
